{
  "name": "tapw",
  "private": true,
  "version": "0.0.0",
  "author": "David Adamson",
  "type": "commonjs",
  "scripts": {
    "dev": "concurrently --kill-others \"vite\" \"arri dev\"",
    "check:types": "vue-tsc --noEmit",
    "check:format": "prettier --ignore-path .gitignore --check .",
    "check:lint": "eslint --ext .vue,.ts --ignore-path .gitignore .",
    "test:e2e": "cypress run --browser chrome",
    "build": "arri build && vite build"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@arrirpc/client": "^0.58.1",
    "@arrirpc/schema": "^0.58.1",
    "@arrirpc/server": "^0.58.1",
    "@fontsource-variable/inter": "^5.0.20",
    "@fontsource-variable/jetbrains-mono": "^5.0.21",
    "@iconify-icons/uil": "^1.2.3",
    "@iconify/vue": "^4.1.2",
    "arri": "^0.58.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "radix-vue": "^1.9.2",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.4.35",
    "vue-router": "^4.4.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.13",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cypress": "^13.13.2",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.4.0",
    "eslint-plugin-vue": "^9.27.0",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.7",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vue-tsc": "^2.0.29"
  }
